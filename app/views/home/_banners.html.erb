<section class="banner-section-ten">
	<div class="banner-carousel owl-carousel owl-theme default-nav">
		<div class="item">
			<video class="htms-video" poster="<%= image_url('banner-home2.png') %>" autoplay loop muted playsinline>
				<source type="video/mp4" src="<%= asset_url('triadipa_motion_banner.mp4') %>">
			</video>
		</div>
		<div class="item banner-section-three -type-17" style="background-image:url('<%= image_url("banner-home2.png") %>');">
			<div class="auto-container">
				<div class="row align-items-center">
					<div class="content-column col-lg-12 col-md-12 col-sm-12">
						<div class="inner-column">
							<div class="text-start title-box wow fadeInUp">
								<h3>
									Find a Doctor and Schedule <br/>
									an Appointment at Tria Dipa Hospital
								</h3>
								<div class="text">
									The doctors and staff at Tria Dipa Hospital are eager to help you with any medical inquiries that you may have.
								</div>
							</div>
							<%= render "search_form" %>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</section>
